local VerifiedScripts = Window:MakeTab({
	Name = "Verified Scripts",
	Icon = "rbxassetid://7072706576",
	PremiumOnly = false
})

VerifiedScripts:AddButton({
	Name = "ARCTIC  (Da hood)",
	Callback = function()
		loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/PolarWasHere/Arctic/main/Arctic"))()
	end    
})

VerifiedScripts:AddButton({
	Name = "Bowling Always Strike press 'q' (Ro Bowling)",
	Callback = function()

		local Mouse = LocalPlayer:GetMouse()
		local Character = LocalPlayer.Character
		local HRP = Character.HumanoidRootPart

		function GetLanes()
			for i,v in pairs(game.Workspace.FunctionalAssets.BowlingLanes.Lanes:GetChildren()) do
				for a,b in pairs(v.Scoring.Screen2.BowlingLaneBoard.BoardGui.ImageLabel:GetChildren()) do
					if b.Name == game.Players.LocalPlayer.Name then
						return b.Parent.Parent.Parent.Parent.Parent.Parent
					end
				end
			end
		end

		Mouse.KeyDown:Connect(function(Key)
			Key = Key:lower()
			if Key == "q" then
				for i,v in pairs(GetLanes().FunctioningAssets.Pins:GetChildren()) do
					v.Rotation = Vector3.new(90,0,0)
				end
			end
		end)
	end    
})

VerifiedScripts:AddButton({
	Name = "KJ HUB (Anime Fighters Simulator)",
	Callback = function()
		loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/KiJinGaming/FreeScript/main/KJHub.lua"))();
	end    
})

VerifiedScripts:AddButton({
	Name = "Backdoor.exe",
	Callback = function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/iK4oS/backdoor.exe/master/source.lua'),true))()
	end    
})

