-- Games Holder

local dropdown = FeaturedGames:Dropdown("Featured Games", {"Anime Fighters Simulator","A 0ne Piece Game","Piano Game"},"","Dropdown", function(t)
	if t == "Anime Fighters Simulator" then
		game:GetService("TeleportService"):Teleport(6299805723, game.Players.LocalPlayer)
	elseif t == "A 0ne Piece Game" then
		game:GetService("TeleportService"):Teleport(8396586868, game.Players.LocalPlayer)
	elseif t ==  "Piano Game" then
		game:GetService("TeleportService"):Teleport(233727153, game.Players.LocalPlayer)
	end
end)

local dropdown = BackDooredGames:Dropdown("Backdoored Games", {"Survive the killer clown!"},"", "Dropdown", function(bdg)
	if bdg == "Survive the killer clown!" then
		game:GetService("TeleportService"):Teleport(6131060067, game.Players.LocalPlayer)
	end
end)

