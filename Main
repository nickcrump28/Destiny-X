-- Main UI

local Lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/nickcrump28/cool-ui-code/main/Destiny"))()

-- Friend Join Notification

game.Players.PlayerAdded:Connect(function(player)
	if player:IsFriendsWith(game.Players.LocalPlayer.UserId) then
		Lib:Notification("Friend Joined!", player.Name.." has joined the game!")
		if player.Name == "just_amistakelol" then
			Lib:Notification("Girlfriend Joined!", player.Name.." The Love of my life has joined the game!")
		end
	end
end)

Lib:Notification("Loaded!", "Destiny X has been loaded, thank you")

