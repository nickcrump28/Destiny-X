-- Buttons

Buttons:Button("Rejoin", function()
	local ts = game:GetService("TeleportService")
	local p = game:GetService("Players").LocalPlayer

	ts:Teleport(game.PlaceId, p)
end)

Buttons:Button("Walk On Walls", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/specowos/scriptsforvideos1/main/walkonwalls.lua',true))()
	Lib:Notification("Walk On Walls", "Script Activated")
end)

-- toggles and inputs

local toggle = Toggles:Toggle("Day/Night", false, "Day/Night", function(value)
	if value == true then
		game.Lighting.TimeOfDay = 0
		Lib:Notification("Time Of Day Changed", "The Time Has been changed to Night")
	elseif value == false then
		game.Lighting.TimeOfDay = 12
		Lib:Notification("Time Of Day Changed", "The Time Has been changed to Day")
	end
end)

Toggles:Textbox("Input Something Here",  true, function(code)
	Lib:Notification("Destiny X System ", code)
end)

